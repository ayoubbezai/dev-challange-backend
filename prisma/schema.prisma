    generator client {
      provider = "prisma-client-js"
    }

    datasource db {
      provider = "mongodb"
    }

model User {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  full_name  String
  role       String
  nick_name  String   @unique
  email      String   @unique
  password   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
